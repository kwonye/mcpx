{
  "name": "@kwonye/mcpx",
  "version": "0.1.3",
  "description": "HTTP-first MCP gateway and cross-client installer",
  "keywords": [
    "mcp",
    "cli",
    "gateway"
  ],
  "homepage": "https://github.com/kwonye/mcpx#readme",
  "bugs": {
    "url": "https://github.com/kwonye/mcpx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwonye/mcpx.git"
  },
  "license": "MIT",
  "author": "Will Kwon",
  "type": "module",
  "main": "index.js",
  "bin": {
    "mcpx": "dist/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx src/cli.ts",
